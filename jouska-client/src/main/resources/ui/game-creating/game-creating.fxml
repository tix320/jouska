<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="184.0" prefWidth="374.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.gitlab.tixtix320.jouska.client.ui.GameCreatingController">
	<children>
		<ChoiceBox fx:id="playersCountChoice" layoutX="112.0" layoutY="47.0" prefWidth="150.0"/>
		<Button fx:id="createButton" layoutX="112.0" layoutY="127.0" mnemonicParsing="false" onAction="#create"
				prefHeight="25.0" prefWidth="150.0" text="Create"/>
		<TextField fx:id="gameNameTextField" layoutX="113.0" layoutY="14.0" promptText="Name"/>
		<Button fx:id="backButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#back"
				prefHeight="25.0" prefWidth="54.0" text="Back"/>
	</children>
</AnchorPane>
