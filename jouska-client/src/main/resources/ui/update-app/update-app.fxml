<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
			xmlns:fx="http://javafx.com/fxml/1"
			fx:controller="com.github.tix320.jouska.client.ui.controller.UpdateAppController">
	<children>
		<StackPane prefHeight="150.0" prefWidth="200.0"/>
		<Group/>
		<ProgressIndicator fx:id="loadingIndicator" layoutX="275.0" layoutY="150.0" prefHeight="58.0" prefWidth="59.0"
						   progress="0.0"/>
		<Label fx:id="messageLabel" alignment="CENTER" layoutX="47.0" layoutY="47.0" prefHeight="117.0"
			   prefWidth="507.0">
			<font>
				<Font size="24.0"/>
			</font>
		</Label>
		<Button fx:id="updateButton" layoutX="200.0" layoutY="248.0" mnemonicParsing="false" onMouseClicked="#updateApp"
				prefHeight="68.0" prefWidth="201.0" text="Update">
			<font>
				<Font size="24.0"/>
			</font>
		</Button>
	</children>
</AnchorPane>
