<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="root" prefHeight="682.0" prefWidth="776.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gitlab.tixtix320.jouska.client.ui.GameController">
    <children>
        <GridPane fx:id="gameGrid" layoutX="19.0" layoutY="71.0" prefHeight="594.0" prefWidth="739.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
        </GridPane>
        <Label fx:id="timeIndicator" alignment="CENTER" layoutX="648.0" prefHeight="29.0" prefWidth="110.0" text="Time">
            <font>
                <Font size="28.0"/>
            </font>
        </Label>
        <Label alignment="CENTER" layoutX="14.0" prefHeight="29.0" prefWidth="110.0" text="Turn:">
            <font>
                <Font size="28.0"/>
            </font>
        </Label>
        <Circle fx:id="turnIndicator" fill="DODGERBLUE" layoutX="124.0" layoutY="20.0" radius="16.0" stroke="BLACK"
                strokeType="INSIDE"/>
    </children>
</AnchorPane>
