<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<AnchorPane maxWidth="500.0" minWidth="250.0" prefHeight="80.0" stylesheets="@confirm-notification.css"
			xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<Button id="acceptButton" layoutX="25.0" layoutY="42.0" mnemonicParsing="false" onAction="#onAccept"
				styleClass="choiceButton" text="Accept" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="25.0"/>
		<Label id="notificationTextLabel" fx:id="notificationTextLabel" layoutX="123.0" layoutY="14.0" text="Text"
			   textFill="#fffc42" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
			   AnchorPane.topAnchor="10.0"/>
		<Button id="rejectButton" layoutX="193.0" layoutY="42.0" mnemonicParsing="false" onAction="#onReject"
				styleClass="choiceButton" text="Reject" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="25.0"/>
	</children>
</AnchorPane>
